{
  "name": "plog-frontend",
  "version": "0.1.0",
  "description": "Frontend for plog",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "test-with-coverage": "react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/registerServiceWorker.js",
    "test-with-coveralls": "react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/registerServiceWorker.js && cat ./coverage/lcov.info | coveralls",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf build",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edgarmueller/plog-frontend"
  },
  "author": "Edgar Mueller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edgarmueller/plog-frontend/issues"
  },
  "homepage": "http://plog-frontend.herokuapp.com/",
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!src/registerServiceWorker.js",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/test/**",
      "!**/public/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**/__mocks__/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "html"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "exclude": [
      "src/index.js",
      "src/components/configureStore.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.2",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "redux-mock-store": "^1.5.3"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.0",
    "@material-ui/icons": "^3.0.1",
    "awesome-debounce-promise": "^1.0.0",
    "axios": "^0.21.1",
    "brace": "^0.11.1",
    "dropbox": "^8.2.0",
    "highlight.js": "^10.4.1",
    "history": "^4.7.2",
    "jwt-decode": "^2.1.0",
    "katex": "^0.9.0-alpha2",
    "lodash": "^4.17.20",
    "markdown-it": "^12.3.2",
    "markdown-it-katex": "^2.0.3",
    "moment": "^2.20.1",
    "org": "^0.2.0",
    "prop-types": "^15.5.8",
    "radium": "^0.23.0",
    "react": "^16.6.3",
    "react-ace": "^6.2.0",
    "react-dom": "^16.6.3",
    "react-hotkeys": "^1.1.4",
    "react-markdown": "^3.3.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^2.1.1",
    "react-spinners": "^0.9.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
